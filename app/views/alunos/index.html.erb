<p id="notice"><%= notice %></p>

<h1>Alunos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Registro cbj</th>
      <th>Telefone1</th>
      <th>Telefone2</th>
      <th>Email</th>
      <th>Cpf</th>
      <th>Observacoes</th>
      <th>Rg</th>
      <th>Rua</th>
      <th>Numero residencia</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Cep</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.nome %></td>
        <td><%= aluno.registro_cbj %></td>
        <td><%= aluno.telefone1 %></td>
        <td><%= aluno.telefone2 %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.cpf %></td>
        <td><%= aluno.observacoes %></td>
        <td><%= aluno.rg %></td>
        <td><%= aluno.rua %></td>
        <td><%= aluno.numero_residencia %></td>
        <td><%= aluno.bairro %></td>
        <td><%= aluno.cidade %></td>
        <td><%= aluno.estado %></td>
        <td><%= aluno.cep %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno', new_aluno_path %>
