<p id="notice"><%= notice %></p>

<div class="panel panel-default">
  <div class="panel-heading"><%= @entidade.nome %></div>
  <div class="panel-body">
    <p>
      <strong>Telefone1:</strong>
      <%= @entidade.telefone1 %>
    </p>

    <p>
      <strong>Telefone2:</strong>
      <%= @entidade.telefone2 %>
    </p>

    <p>
      <strong>Cnpj:</strong>
      <%= @entidade.cnpj %>
    </p>

    <p>
      <strong>Rua:</strong>
      <%= @entidade.rua %>
